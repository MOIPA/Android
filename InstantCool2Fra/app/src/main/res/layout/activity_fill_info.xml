                android:gravity="center_horizontal"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/iv_user_r_icon5"
                        android:src="@mipmap/user_r_icon5"
                        android:layout_width="50dp"
                        android:layout_height="50dp" />

                    <ImageView
                        android:id="@+id/iv_user_select5"
                        android:layout_width="20dp"
                        android:layout_height="20dp"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginEnd="21dp"
                        android:scaleType="centerInside"
                        android:src="@mipmap/un_selected" />
                </RelativeLayout>
                <TextView
                    android:layout_marginTop="10dp"
                    android:text="可爱"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:gravity="center_horizontal"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/iv_user_r_icon6"
                        android:src="@mipmap/user_r_icon6"
                        android:layout_width="50dp"
                        android:layout_height="50dp" />
                    <ImageView
                        android:id="@+id/iv_user_select6"
                        android:layout_toRightOf="@id/iv_user_r_icon6"
                        android:scaleType="centerInside"
                        android:src="@mipmap/un_selected"
                        android:layout_width="20dp"
                        android:layout_height="20dp" />
                </RelativeLayout>
                <TextView
                    android:layout_marginTop="10dp"
                    android:text="可爱"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
