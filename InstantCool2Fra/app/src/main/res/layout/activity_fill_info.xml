<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tr.instantcool2.Activity.FillInfoActivity">

    <com.example.tr.instantcool2.IndicatorView.TopBarIndicatorView
        android:layout_width="368dp"
        android:layout_height="56dp"
        android:id="@+id/top_bar_container_fill_information"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp">

    </com.example.tr.instantcool2.IndicatorView.TopBarIndicatorView>
    <LinearLayout
        android:orientation="vertical"
        android:layout_below="@id/top_bar_container_fill_information"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp">

        <EditText
            android:background="@color/white"
            android:layout_gravity="center_horizontal"
            android:id="@+id/et_fill_information"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:drawableLeft="@mipmap/accountfilling"
            android:drawablePadding="15dp"
            android:hint="您的昵称"
            android:minHeight="40dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:singleLine="true" />

        <LinearLayout
            android:paddingLeft="40dp"
            android:paddingRight="40dp"
            android:layout_margin="15dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/iv_user_r_icon1"
                        android:src="@mipmap/user_r_icon1"
                        android:layout_width="50dp"
                        android:layout_height="50dp" />
                    <ImageView
                        android:id="@+id/iv_user_select1"
                        android:layout_toRightOf="@id/iv_user_r_icon1"
                        android:scaleType="centerInside"
                        android:src="@mipmap/un_selected"
                        android:layout_width="20dp"
                        android:layout_height="20dp" />
                </RelativeLayout>

                <TextView
                    android:layout_marginTop="10dp"
                    android:text="可爱"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:gravity="center_horizontal"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/iv_user_r_icon2"
                        android:src="@mipmap/user_r_icon2"
                        android:layout_width="50dp"
                        android:layout_height="50dp" />
                    <ImageView
                        android:id="@+id/iv_user_select2"
                        android:layout_toRightOf="@id/iv_user_r_icon2"
                        android:scaleType="centerInside"
                        android:src="@mipmap/un_selected"
                        android:layout_width="20dp"
                        android:layout_height="20dp" />
                </RelativeLayout>
                <TextView
                    android:layout_marginTop="10dp"
                    android:text="可爱"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:gravity="center_horizontal"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/iv_user_r_icon3"
                        android:src="@mipmap/user_r_icon3"
                        android:layout_width="50dp"
                        android:layout_height="50dp" />
                    <ImageView
                        android:id="@+id/iv_user_select3"
                        android:layout_toRightOf="@id/iv_user_r_icon3"
                        android:scaleType="centerInside"
                        android:src="@mipmap/un_selected"
                        android:layout_width="20dp"
                        android:layout_height="20dp" />
                </RelativeLayout>
                <TextView
                    android:layout_marginTop="10dp"
                    android:text="可爱"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:paddingLeft="40dp"
            android:paddingRight="40dp"
            android:layout_margin="15dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:gravity="center_horizontal"
                android:orientation="vertical"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/iv_user_r_icon4"
